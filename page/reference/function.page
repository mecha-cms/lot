### Categories

[[e]]
$categories = [
    'Array & Object' => 'a,alt,any,concat,extend,find,has,is,map,not,o,pluck,shake,z',
    'Boolean' => 'eq,ge,gt,le,lt,ne',
    'File' => 'd,g,i,r',
    'Function' => 'fn,y',
    'Number' => 'b,m,q',
    'String' => 'c,candy,c2f,e,f,format,f2c,h,l,n,p,s,t,u,v,w,x',
    'Others' => 'fail,j,k,test'
];

foreach ($categories as $k => $v) {
    echo '#### ' . $k . "\n";
    $names = explode(',', $v);
    sort($names);
    foreach ($names as $vv) {
        echo "\n - [link:/reference/function/" . $vv . ']';
    }
    echo "\n\n";
}
[[/e]]