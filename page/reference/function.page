foreach ($categories as $k => $v) {
    echo '#### ' . $k . "\n";
    $names = explode(',', $v);
    sort($names);
    foreach ($names as $vv) {
        echo "\n - [link:/reference/function/" . $vv . ']';
    }
    echo "\n\n";
