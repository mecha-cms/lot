---
@version: 2.2
@description: Convert property name to file name.
@lot:
- string $value, $accent = false
@example: |
  echo p2f('foo');
  echo p2f('__foo');
  echo p2f('__foo\bar');
  echo p2f('fooBar');
  echo p2f('foo_bar');
  echo p2f('foo_Bar');
  echo p2f('FOO_BAR');
@result: |
  [[e]]
  echo p2f('foo') . "\n";
  echo p2f('__foo') . "\n";
  echo p2f('__foo\bar') . "\n";
  echo p2f('fooBar') . "\n";
  echo p2f('foo_bar') . "\n";
  echo p2f('foo_Bar') . "\n";
  echo p2f('FOO_BAR');
  [[/e]]
...
