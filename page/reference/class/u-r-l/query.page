---
@version: [current]
@description: Return the URL query.
@lot:
- void
- string $separator = '&'
- string $separator = '&', array $q = []
@static: 2
@example: |
  $A = $url->query;
  $B = $url->query();
  $C = $url->query('&amp;');
  $D = $url->query(';', ['foo', => '#']);
@result: |
  $A = '[[e]]echo (new URL('?foo=bar&baz=qux'))->query;[[/e]]';
  $B = '[[e]]echo (new URL('?foo=bar&baz=qux'))->query();[[/e]]';
  $C = '[[e]]echo (new URL('?foo=bar&baz=qux'))->query('&amp;');[[/e]]';
  $D = '[[e]]echo (new URL('?foo=bar&baz=qux'))->query(';', ['foo' => '#']);[[/e]]';
...