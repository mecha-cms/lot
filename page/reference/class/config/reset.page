---
@version: [2, current]
@description: Remove global state item.
@lot:
  - void
  - string $key = null
  - array $keys = []
@static: 1
@example: |
  // Remove all items
  Config::reset();
  // Remove `foo` item
  Config::reset('foo');
  // Remove `foo` and `bar` item
  Config::reset(['foo', 'bar']);
...