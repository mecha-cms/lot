---
@version: [2, current]
@description: Move to the next item.
@lot:
- int $skip = 0
@example: |
  $array = [
      'foo' => 0,
      'bar' => 1,
      'baz' => 2,
      'qux' => 3
  ];

  $A = Anemon::from($array)->next()->current();
  $B = Anemon::from($array)->next()->next()->current();
  $C = Anemon::from($array)->next(1)->current();
@result: |
  $A = 1;
  $B = 2;
  $C = 2;
...