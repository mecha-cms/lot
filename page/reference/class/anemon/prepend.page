---
@version: [2, current]
@description: Insert element to the start of array.
@lot: $value, $key = null
@example: |
  $array = [
      'foo' => 0,
      'bar' => 1,
      'baz' => 2,
      'qux' => 3
  ];

  $A = Anemon::from($array)->prepend('#')->get();
  $B = Anemon::from($array)->prepend('#', 'a')->get();
@result: |
  $A = [
      0 => '#',
      'foo' => 0,
      'bar' => 1,
      'baz' => 2,
      'qux' => 3
  ];

  $B = [
      'a' => '#',
      'foo' => 0,
      'bar' => 1,
      'baz' => 2,
      'qux' => 3
  ];
...