---
@version: [2, current]
@description: Move to the <var>N</var> item.
@lot:
- int $i
- string $key
@example: |
  $array = [
      'foo' => 10,
      'bar' => 11,
      'baz' => 12,
      'qux' => 13
  ];

  $A = Anemon::from($array)->to('bar')->current();
  $B = Anemon::from($array)->to(2)->current();
@result: |
  $A = 11;
  $B = 12;
...