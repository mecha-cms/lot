---
@version: [2, current]
@description: Move to the <var>N</var> item.
@lot:
  - $index
  - $key
@example: |
  $array = [
      'foo' => 0,
      'bar' => 1,
      'baz' => 2,
      'qux' => 3
  ];

  $A = Anemon::eat($array)->to('bar')->current();
  $B = Anemon::eat($array)->to(2)->current();
@result: |
  $A = 1;
  $B = 2;
...