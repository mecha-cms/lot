---
@version: [2]
@description: Get cache ID.
@lot: string $from, $fail = -1
@static: 1
@example: |
  // Get cache ID of `.\path\to\file.php`
  $id = Cache::ID('.\path\to\file.php');

  $hash = md5(File::open('.\path\to\file.php')->read());
  if (
      is_string($id) && $id !== $hash ||
      $id < File::inspect('.\path\to\file.php', 'update')
  ) {
      // Expired!
  }
