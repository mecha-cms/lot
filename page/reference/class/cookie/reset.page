---
@version: [2, current]
@description: Remove a cookie.
@lot:
  - void
  - string $key = null
  - array $keys = []
@static: 1
@example: |
  // Remove all cookies
  Cookie::reset();
  // Remove `foo` and `bar` cookie
  Cookie::reset(['foo', 'bar']);
  // Remove `foo` cookie
  Cookie::reset('foo');
...