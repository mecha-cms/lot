---
@version: [2, current]
@description: Convert PHP array into URL query string.
@lot:
- array $in
@static: 1
@example: |
  $A = To::query([
      'a' => 1,
      'b' => true,
      'c' => 'true',
      'd' => false,
      'e' => 'false',
      'f' => null,
      'g' => 'null',
      'h' => "",
      'i' => [
          'j' => [
              'k' => [1, 2],
              'l' => 'mno'
          ]
      ]
  ]);
@result: |
  $A = '[[e]]echo To::query([
      'a' => 1,
      'b' => true,
      'c' => 'true',
      'd' => false,
      'e' => 'false',
      'f' => null,
      'g' => 'null',
      'h' => "",
      'i' => [
          'j' => [
              'k' => [1, 2],
              'l' => 'mno'
          ]
      ]
  ]);[[/e]]';
