---
@version: [2, current]
@description: Convert YAML string into array.
@lot: string $in, string $d = '  ', $e = true
@static: 1
@example: |
  $A = From::YAML('foo: bar');
  $B = From::YAML(
  <<<YAML
  foo: bar
  baz: qux
  YAML
  );
  $C = From::YAML(
  <<<YAML
  ---
  foo: bar
  baz: qux
  ---
  foo: bar
  baz: qux
  YAML
  );
@result: |
  $A = ['foo' => 'bar'];
  $B = [
      'foo' => 'bar',
      'baz' => 'qux'
  ];
  $C = [
      0 => [
          'foo' => 'bar',
          'baz' => 'qux'
      ],
      1 => [
          'foo' => 'bar',
          'baz' => 'qux'
      ]
  ];
...